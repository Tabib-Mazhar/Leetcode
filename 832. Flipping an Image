class Solution(object):
    def flipAndInvertImage(self, image):
        
        for i in range(len(image)):

            image[i].reverse()
            
        
            for j in range(len(image[i])):
                    # print(image[i][j])
                if(image[i][j]):
                        image[i][j]=0
                else:
                        image[i][j]=1

        return(image)

            
        """
        :type image: List[List[int]]
        :rtype: List[List[int]]
        """
        
