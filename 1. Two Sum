class Solution(object):
    def twoSum(self, nums, target):
        
        result = []
        for i in range(0,len(nums)):
            new_number = target-nums[i]
            if(new_number in nums):
                index = nums.index(new_number)
                if(index !=i):
                    result.append(index)
                    result.append(i)
                    return result 

        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        
